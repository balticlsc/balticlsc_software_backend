{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "LogFile": "logs/baltic-server-{Date}.txt",
  "Debug": "false",
  "HTTPS": {
    "Enable": "false"
  },
  "CORS": {
    "Enable": "false",
    "AllowSpecificOrigin": [
      "http://balticlsc.eu",
      "https://balticlsc.eu"
    ]
  },
  "ConsulSettings": {
    "Enabled": "true",
    "Host": "http://172.17.0.1",
    "Port": 8500,
    "ServiceName": "BalticLSC.Service",
    "ServicePort": 5001,
    "UseSSL": "true",
    "CheckService": "true",
    "TLSSkipVerify": "true",
    "ServiceHealthCheckTimeOut": 3,
    "ServiceHealthCheckInterval": 10,
    "DeregisterCriticalServiceAfter": 20
  }
}
  
